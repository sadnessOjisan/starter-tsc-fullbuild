{
  "name": "starter-tsc-fullbuild",
  "version": "1.0.0",
  "description": "TSC + Webpack のReactプロジェクトで1ファイルバンドルをする",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc src/main.ts --outDir dist",
    "lint:debug": "eslint --debug 'src/**/*.{js,ts}'",
    "lint:ci": "eslint 'src/**/*.{js,ts}'",
    "lint": "eslint --fix 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "dev": "webpack-dev-server",
    "typecheck": "yarn tsc -p . --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadnessOjisan/starter-tsc-fullbuild.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sadnessOjisan/starter-tsc-fullbuild/issues"
  },
  "homepage": "https://github.com/sadnessOjisan/starter-tsc-fullbuild#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.2.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn format"
    ]
  }
}
